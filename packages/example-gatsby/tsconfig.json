{
	"include": ["./src/", "./gatsby-*.js"],
	"compilerOptions": {
		"noEmit": true,
		"strict": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"isolatedModules": true,
		"jsx": "react",
		"allowJs": true,
		"checkJs": true,
		"allowSyntheticDefaultImports": true
	}
}
